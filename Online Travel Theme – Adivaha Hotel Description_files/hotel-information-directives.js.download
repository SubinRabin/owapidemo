                            document.getElementById(prefix + "rooms").value = $scope.noofrooms;
                        }
                    };
                    $scope.decreasenoofrooms = function() {
                        if ($scope.noofrooms > 1) {
                            $scope.noofrooms--;
                            document.getElementById(prefix + "rooms").value = $scope.noofrooms;
                        }
                    };

                    function showGuestRoom() {
                        var totalGuest = 0;
