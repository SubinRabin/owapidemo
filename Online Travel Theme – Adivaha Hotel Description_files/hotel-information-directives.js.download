                            } else {
                                jQuery('#moreInfo_' + relratecode).hide();
                                jQuery(this).attr('rel', '0');
                            }
                        })
                    };

                    function suggestedHotels() {
                        var param = "action=Show_Hotel_Suggestions&hotelId=" + $scope.hotelId;
                        var url = actionUrl + '?' + param;
                        $http.get(url).success(function(response) {
                            $scope.Hotel_Suggestions = response.result;
                        });
                    };

                    function getTripAdvisorReview() {
                        var param = "action=getTripAdvisorReview&hotelId=" + $scope.hotelId + "&language=" + $scope.language;
                        var url = actionUrl + '?' + param;
                        $http.get(url).success(function(response) {
                            jQuery('#tripdiv').html(response);
                        });
